{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Google": {
    "ClientId": "YOUR_CLIENT_ID.apps.googleusercontent.com",
    "ClientSecret": "YOUR_CLIENT_SECRET",
    "RedirectUri": "http://localhost:3000/auth/callback",
    "ProjectId": "YOUR_GOOGLE_CLOUD_PROJECT_ID"
  },
  "AnalyticsWorker": {
    "Enabled": true,
    "UserId": "default-user",
    "PropertyId": "YOUR_GA4_PROPERTY_ID",
    "Schedule": "0 0 * * *",
    "Dimensions": [ "date", "country" ],
    "Metrics": [ "activeUsers", "sessions" ]
  },
  "ConnectionStrings": {
    "PostgresConnection": "Host=localhost;Database=oauth_login;Username=postgres;Password=your_password"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "analytics-reports"
  },
  "KafkaAnalyticsWorker": {
    "Enabled": true,
    "UserId": "default-user",
    "PropertyId": "YOUR_GA4_PROPERTY_ID",
    "Schedule": "0 0 * * *",
    "Dimensions": [ "date", "country" ],
    "Metrics": [ "activeUsers", "sessions" ]
  },
  "KafkaHttpSinkWorker": {
    "Enabled": false,
    "UserId": "default-user",
    "PropertyId": "YOUR_GA4_PROPERTY_ID",
    "Schedule": "0 30 * * *",
    "Dimensions": [ "date", "country" ],
    "Metrics": [ "activeUsers", "sessions" ],
    "HttpSinkUrl": "http://localhost:8080/topics/analytics-reports"
  },
  "KafkaDataProcessor": {
    "Enabled": true,
    "Schedule": "*/5 * * * *",
    "MaxMessagesPerBatch": 100,
    "StoreInFirebase": true,
    "StoreInBigQuery": true
  },
  "KafkaJdbcSinkWorker": {
    "Enabled": false,
    "Schedule": "*/10 * * * *",
    "MaxMessagesPerBatch": 100,
    "JdbcSinkUrl": "http://localhost:8080/kafka-jdbc-sink",
    "JdbcConnectionString": "jdbc:postgresql://localhost:5432/oauth_login?user=postgres&password=your_password"
  }}

